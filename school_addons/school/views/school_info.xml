<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="school_form_view" model = "ir.ui.view">
        <field name="name">school details</field>
        <field name="model">school.table.schools</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="school_number"/>
                        <field name="address"/>
                        <field name="win_percentage"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="school_tree_view" model = "ir.ui.view">
        <field name="name">school info</field>
        <field name="model">school.table.schools</field>
        <field name="arch" type="xml">
            <tree>
                        <field name="name"/>
                        <field name="school_number"/>
                        <field name="address"/>
                        <field name="win_percentage"/>
            </tree>
        </field>
    </record>

</odoo>