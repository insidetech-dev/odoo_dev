<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_college">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h2>college details</h2>
                <table style="width:100%">
                    <tr>
                        <th>name</th>
                        <th>college number</th>
                        <th>address</th>
                    </tr>
                    <tr>
                        <td>
                            <t t-esc="doc.name"/>
                        </td>
                        <td>
                            <t t-esc="doc.college_number"/>
                        </td>
                        <td>
                            <t t-esc="doc.address"/>
                        </td>
                    </tr>
                </table>
            </t>
        </t>
    </template>

</odoo>